{
	"name": "Disable YouTube Watch Later Autoplay",
	"description": "Watch individual videos that you saved for later without autoplaying all of them.",
	"version": "1.0.4",
	"manifest_version": 3,
	"icons": {
		"16": "icon-16.png",
		"32": "icon-32.png",
		"48": "icon-48.png",
		"128": "icon-128.png"
	},
	"permissions": [
		"scripting"
	],
	"host_permissions": [
		"https://*.youtube.com/*"
	],
	"content_scripts": [
		{
			"matches": [
				"https://*.youtube.com/*"
			],
			"js": [
				"content.js"
			]
		}
	]
}